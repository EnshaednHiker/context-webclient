{
  "name": "context-webclient",
  "version": "1.0.0",
  "description": "web client for the context content annotation service",
  "main": "index.js",
  "scripts": {
    "open-cypress": "./node_modules/.bin/cypress open",
    "start": "webpack-dev-server",
    "test": "./node_modules/.bin/cypress run",
    "ci-start": "webpack-dev-server --env=production",
    "build": "webpack -p",
    "production-build": "npm run build -- --env=production",
    "local-build": "npm run build -- --env=local",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "ci": "git add . && git commit -m \"CI config change\" && git push",
    "sleep": "sleep 2m",
    "unit-test": "jest"
  },
  "engines": {
    "node": "8.11.1"
  },
  "author": "EnshaednHiker",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "director": "^1.2.8",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.5",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "get-port": "^3.1.0",
    "jsonwebtoken": "^8.2.1",
    "node": "^8.11.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-if": "^2.2.2",
    "react-modal": "^3.4.4",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-scroll": "^1.7.9",
    "redux": "^3.7.2",
    "redux-pack": "^0.1.5",
    "redux-thunk": "^2.2.0",
    "split-at": "^1.2.0",
    "superagent": "^3.8.3",
    "typed.js": "^2.0.6"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "cypress": "^1.4.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-cypress": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "jest-enzyme": "^5.0.3",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "react-test-renderer": "^16.3.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "style-loader": "^0.18.2",
    "wait-on": "^2.1.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EnshaednHiker/context-webclient.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupTests.js",
    "moduleNameMapper": {
      "~(.*)$": "<rootDir>/$1"
    }
  }
}
