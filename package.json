{
  "name": "context-webclient",
  "version": "1.0.0",
  "description": "web client for the context content annotation service",
  "main": "index.js",
  "scripts": {
    "postinstall": "node nightwatch.conf.js",
    "test": "./node_modules/.bin/nightwatch --env local",
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "production-build": "npm run build -- --env=production",
    "local-build": "npm run build -- --env=local",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "ci": "git add . && git commit -m \"CI config change\" && git push"
  },
  "engines": {
    "node": "6.10.2"
  },
  "author": "EnshaednHiker",
  "license": "ISC",
  "dependencies": {
    "director": "^1.2.8",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "get-port": "^3.1.0",
    "jsonwebtoken": "^8.1.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-if": "^2.2.1",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.6",
    "react-scroll": "^1.7.5",
    "redux": "^3.7.2",
    "redux-pack": "^0.1.5",
    "redux-thunk": "^2.2.0",
    "split-at": "^1.2.0",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^2.30.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nightwatch": "^0.9.16",
    "redux-devtools-extension": "^2.13.2",
    "selenium-download": "^2.0.10",
    "style-loader": "^0.18.2",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.9.7"
  },
  "repository": {
